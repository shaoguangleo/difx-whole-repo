C     Include file for CLOCKHIST
C
      INTEGER  MAXEXP, MAXSTA, MAXBAS
      PARAMETER (MAXEXP=500)
      PARAMETER (MAXSTA=11)
      PARAMETER (MAXBAS=MAXSTA*MAXSTA)
      INTEGER  NEXP, NBAS(MAXEXP), NREF(MAXEXP), NACC
      INTEGER  ISTA1(MAXBAS,MAXEXP), ISTA2(MAXBAS,MAXEXP)
      INTEGER  NPLT, PLTSTA(MAXSTA), IREF
      REAL     MAXAMP(MAXBAS,MAXEXP), DELAY(MAXBAS,MAXEXP)
      REAL     RATE(MAXBAS,MAXEXP), TIME(MAXBAS,MAXEXP)
      REAL     UXMIN, UXMAX, UYMIN, UYMAX, UMINAMP, UMAXRATE
      CHARACTER NAMES(MAXSTA)*2, REFSTA*2, DATAFILE(MAXEXP)*132
      CHARACTER SOURCE(MAXBAS,MAXEXP)*12
      CHARACTER FILELIST*80, ACCFILE*132, PLTFILE*80
C
      COMMON  / CKD / NEXP, NBAS, NREF, NACC, ISTA1, ISTA2, NPLT, 
     1                PLTSTA, IREF, MAXAMP, DELAY, RATE, TIME, 
     2                UXMIN, UXMAX, UYMIN, UYMAX,
     3                UMINAMP, UMAXRATE
      COMMON  / CKC / NAMES, REFSTA, DATAFILE, SOURCE,
     1                FILELIST, ACCFILE, PLTFILE
C
C     Variables for reading lines of data.  Put in common to
C     avoid compiler complaints about unused variables.
C     WORD is big here to be able to get file names.
C
      INTEGER    MWORDS
      PARAMETER  (MWORDS=45)
      INTEGER    WLEN(MWORDS), NWORDS
      CHARACTER  WORD(MWORDS)*80
      COMMON / RLNDAT / WLEN, NWORDS
      COMMON / RLNCHR / WORD
C -----------------------------------------------------------------
